# Git
alias gb='git branch '
alias ga='git add '
alias gaa='git add -A'
alias gc='git commit -m '
alias gca='git commit --amend'
alias gd='git diff '
alias gdc='git diff --cached'
alias gds='git diff --staged'
alias gsh='git show '
alias gps='git push '
alias gpso='git push origin '
alias gpl='git pull '
alias gm='git merge '
alias gs='git status '
alias gch='git checkout '
alias gcl='git clone '
alias gl='git log --oneline --decorate --graph --color --summary'
alias gst='git stash'

# Vim
alias v='vim'

# Tmux
alias t='tmux -2u'
